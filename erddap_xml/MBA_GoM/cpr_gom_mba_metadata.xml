                <att name="_FillValue" type="short">32767</att>
                <att name="ioos_category">Identifier</att>
                <att name="long_name">Taxonomic Identification Number</att>
            </addAttributes>
        </dataVariable>
        <dataVariable>
            <sourceName>abundance_per_transect</sourceName>
            <destinationName>abundance_per_transect</destinationName>
